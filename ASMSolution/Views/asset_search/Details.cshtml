@model ASM_UI.Models.asset_registrationViewModel
@{
    ViewBag.Title = "Asset Details";
}
<div id="content">
    <div class="row">
        <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
            <h1 class="page-title txt-color-blueDark">

                <!-- PAGE HEADER -->
                <i class="fa-fw fa fa-pencil-square-o"></i>
                General
                <span>
                    >
                    Asset Detail
                </span>
            </h1>
        </div>
    </div>
    <section id="widget-grid" class="">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget" id="wid-id-3" data-widget-editbutton="false" data-widget-custombutton="false" data-widget-togglebutton="false" data-widget-collapsed="false">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                        <h2>Asset Detail</h2>

                    </header>
                    <!-- widget div-->
                    <div>
                        <!-- widget edit box -->
                        <div class="jarviswidget-editbox">
                            <!-- This area used as dropdown edit box -->

                        </div>
                        <!-- end widget edit box -->
                        <!-- widget content -->
                        <div class="widget-body no-padding">
                            <form class="smart-form">
                                <header>
                                    Asset Detail
                                </header>

                                <fieldset>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>Asset Status</b>
                                            </div>
                                            <div class="col-md-8">
                                                <label class="input">
                                                    @Html.Label("Active", new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_number)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.asset_number, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_name)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.asset_name, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.company_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.company_register, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_merk)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.asset_merk, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_reg_location_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.location_register, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_serial_number)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.asset_serial_number, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_reg_pic_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.pic_register, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.vendor_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.vendor_name, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">

                                            <div class='table-merge'>
                                                <div class="col-md-1">
                                                </div>
                                                <div class="col-md-2">
                                                    <b>@Html.DisplayNameFor(model => model.category_id)</b>
                                                </div>
                                                <div class="col-md-3">
                                                    <label class="input">
                                                        @Html.DisplayFor(model => model.category_register, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                    </label>
                                                </div>

                                                <div class="col-md-1">
                                                </div>
                                                <div class="col-md-2">
                                                    <b>@Html.DisplayNameFor(model => model.asset_description)</b>
                                                </div>
                                                <div class="col-md-3">
                                                    <label class="input">
                                                        @Html.TextAreaFor(model => model.asset_description, new { htmlAttributes = new { @class = "form-control", @readonly = "true", @disable = "true" } })
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.asset_receipt_date)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.asset_receipt_date, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>

                                    <br />
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.location_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.location_asset, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.original_price)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.original_price, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.department_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.department_asset, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.usage_life_time_fiskal)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.usage_life_time_fiskal, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>
                                    <section>
                                        <div class="row">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.employee_id)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.employee_name, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>

                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-2">
                                                <b>@Html.DisplayNameFor(model => model.fis_book_value)</b>
                                            </div>
                                            <div class="col-md-3">
                                                <label class="input">
                                                    @Html.DisplayFor(model => model.fis_book_value, new { htmlAttributes = new { @class = "form-control", @readonly = "true" } })
                                                </label>
                                            </div>
                                        </div>
                                    </section>

                                </fieldset>
                                <footer>
                                    <div class="form-group">
                                        <div class="col-md-7">
                                            @Html.ActionLink("Close", "Index", "asset_registration", htmlAttributes: new { @class = "btn btn-default" })
                                        </div>
                                    </div>
                                </footer>
                            </form>
                        </div>

                    </div>
                </div>
            </article>
        </div>
    </section>
</div>
<p>

</p>
