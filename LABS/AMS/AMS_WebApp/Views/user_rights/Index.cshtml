@model IEnumerable<ASM_UI.Models.ms_user_rights>
@{
    ViewBag.Title = "User Access";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.user_type_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.job_level_id)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.user_type.user_type_name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.job_level.job_level_name)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.user_type_id, id1 = item.job_level_id }) |
                @Html.ActionLink("Details", "Details", new { id = item.user_type_id, id1 = item.job_level_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.user_type_id, id1 = item.job_level_id })
            </td>

        </tr>
    }

</table>
